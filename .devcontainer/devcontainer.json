{
    "dockerComposeFile": [
      "../compose.yml"
    ],
    "service": "server",
    "workspaceFolder": "/workspace",
    "customizations": {
      "vscode": {
        "extensions": [
          "GitHub.copilot",
          "ms-azuretools.vscode-docker",
          "rust-lang.rust-analyzer",
          "tamasfe.even-better-toml",
          "serayuzgur.crates",
          "adpyke.vscode-sql-formatter"
        ],
        "settings": {
          "files.watcherExclude": {
            "**/target/**": true
          }
        }
      }
    }
  }